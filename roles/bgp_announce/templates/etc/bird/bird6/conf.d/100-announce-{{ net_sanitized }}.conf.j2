protocol static '{{ net_sanitized }}' {
  table t_fulltable;
{% if ipv6.interface is defined %}
  route {{ ipv6.net }} via "{{ ipv6.interface }}";
{% else %}
  {%- if ipv6.address is defined %}
  route {{ ipv6.net }} via {{ ipv6.address }};
  {%- else %}
  route {{ ipv6.net }} unreachable;
  {%- endif %}
{% endif %}

}

